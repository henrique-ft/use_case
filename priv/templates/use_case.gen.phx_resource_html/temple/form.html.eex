form_for @changeset, @action, fn f ->
  if @changeset.action do
    div class: "alert alert-danger" do
      p gettext("Oops, something went wrong! Please check the errors below.")
    end
  end
  for {label, input, error} <- @inputs, input do
    raw label
    raw input
    raw error
  end
  div do
    submit gettext("Save")
  end
end
